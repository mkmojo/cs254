*ReverseVideo:		off

xterm*visualBell:	off
xterm*scrollBar:	on
xterm*reverseWrap:	on
xterm*scrollInput:	on
xterm*jumpScroll:	on
xterm*saveLines:	1000
xterm*charClass:	33:48,37:48,45-47:48,64:48
xterm*thickness:	10
xterm*title:		CLIENTHOST
xterm*VT100.Translations: #override Shift <Key>F29: scroll-back(1,line) \n\
				    Shift <Key>F35: scroll-forw(1,line) \n\
				    <Key>F29: scroll-back(1,halfpage) \n\
				    <Key>F35: scroll-forw(1,halfpage)

xload*update:		2

emacs*BitmapIcon:	on
emacs*font:		6x13

xclock*padding:		2
xclock*font:		-adobe-courier-medium-r-*-*-14-*-*-*-*-*-*-*

xman*bothShown:		false
xman*directoryHeight:	200
xman*topBox:		false

xclipboard*font:	6x13

!Netscape.maxImageColors: 124
!Netscape.installColormap: True
